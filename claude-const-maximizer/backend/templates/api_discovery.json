  "discovery_process": {
    "steps": [
      "Identify app type and core functionality",
      "Research competitors and their tech stack",
      "Find relevant APIs for core features",
      "Identify datasets for training/testing",
      "Find open-source components for UI/UX",
      "Evaluate pricing and rate limits",
      "Check documentation quality",
      "Assess integration difficulty",
      "Create integration roadmap"
    ],
    "evaluation_criteria": {
      "pricing": ["Free tier available", "Reasonable pricing", "Scalable costs"],
      "documentation": ["Clear examples", "API reference", "Tutorials available"],
      "integration": ["SDK available", "REST API", "Webhook support"],
      "reliability": ["Uptime SLA", "Support quality", "Community size"],
      "performance": ["Response times", "Rate limits", "Scalability"]
    }
  }
